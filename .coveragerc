[coverage:run]
branch = True

[coverage:report]
show_missing = True

# Elements from GUI and Controller are being omitted in the coverage report
omit = 
    *__init__.py*
    blastsight/__main__.py
    blastsight/view/gui/*.py
    blastsight/view/viewer.py
    blastsight/view/integrableviewer.py
    blastsight/controller/*.py

